{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titleProgram": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "date",
      "required": true
    },
    "end": {
      "type": "date",
      "required": true
    },
    "programType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Workshop",
        "Residency"
      ]
    },
    "mediaProgram": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
